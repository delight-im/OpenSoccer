msgstr "Gran cruze por la derecha."

#: ../../../aa_spieltag_simulation.php:159
msgid "Tolle Flanke von links."
msgstr "Grandioso cruze por la izquierda."

#: ../../../aa_spieltag_simulation.php:160
msgid "Schöne Flanke von links."
msgstr "Gran cruze por la izquierda."

#: ../../../aa_spieltag_simulation.php:161
msgid "Tolle Flanke von rechts."
msgstr "Grandioso cruze por la derecha."

#: ../../../aa_spieltag_simulation.php:162
